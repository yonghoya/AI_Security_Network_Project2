{
  "manifest_version": 3,
  "name": "Phishing Box",
  "description": "Base Level Extension",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    //"default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [{
	  "matches": ["https://*/*"],
	  "js": ["content_scripts.js"]
  }
  ]
}